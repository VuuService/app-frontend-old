{
  "name": "app-frontend",
  "version": "0.0.1",
  "private": true,
  "author": "Ahmet YILDIZ",
  "license": "Vuu Digital",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "openapi": "openapi-generator-cli generate -i api/openapi.json -g typescript-axios -o api/generated --additional-properties=withSeparateModelsAndApi=true,apiPackage=apis,modelPackage=models,supportsES6=true,useSingleRequestParameter=true",
    "vue-i18n-extract": "npx vue-i18n-extract report --vueFiles './src/**/*.?(js|ts|vue)' --languageFiles './src/i18n/*.?(json|yml|yaml|js|ts)' --add"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "dayjs": "^1.11.9",
    "firebase": "^10.3.0",
    "flowbite": "^1.8.1",
    "flowbite-vue": "^0.0.16",
    "pinia": "^2.1.6",
    "vue": "^3.3.4",
    "vue-i18n": "^9.3.0-beta.14-77e850b",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node20": "^20.1.2",
    "@types/node": "^20.5.7",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.15",
    "eslint": "^8.48.0",
    "eslint-plugin-vue": "^9.17.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.29",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.3.3",
    "typescript": "~5.2.2",
    "vite": "^4.4.9",
    "vue-i18n-extract": "^2.0.7",
    "vue-tsc": "^1.8.8"
  }
}
